- if @organization_list.count > 1
  %td.left= a.organization
%td.left= a.asset_subtype
%td.left= a.asset_tag
%td.left= a.description

%td.left= link_to(a.parent.name, inventory_path(a.parent) ) unless a.parent.nil?
%td.left= link_to(a.location.name, inventory_path(a.location) ) unless a.location.nil?
%td.right= format_as_fiscal_year(a.in_service_date.year)
%td.right= format_as_integer(a.age)
%td.center= format_as_decimal(a.reported_condition_rating, 1)
%td.center= a.service_status_type.code unless a.service_status_type.nil?

%td.center= format_as_fiscal_year(a.scheduled_rehabilitation_year)
%td.center= format_as_fiscal_year(a.scheduled_replacement_year)
