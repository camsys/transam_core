<Cell><Data ss:Type="String"><%= format_as_decimal(a.reported_condition_rating, 1) %></Data></Cell>
<Cell><Data ss:Type="String"><%= a.service_status_type.code unless a.service_status_type.nil? %></Data></Cell>

<Cell><Data ss:Type="String"><%= format_as_fiscal_year(a.scheduled_rehabilitation_year) %></Data></Cell>
